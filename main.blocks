tatement name="DO0"><block type="game_show_long_text" id="Da5]k!QDgeZkZe%;Q#7`"><field name="layout">DialogLayout.Full</field><value name="str"><shadow type="text" id="|u.wY~CJgk0([WN4+xE?"><field name="TEXT">Fact 11:Approximately every square mile of ocean has more than 46,000 pieces of plastic floating in it.</field></shadow></value></block></statement><value name="IF1"><block type="logic_compare" id="a|%m`F(o.DB?]:rA=U)t"><field name="OP">EQ</field><value name="A"><shadow type="math_number"><field name="NUM">0</field></shadow><block type="device_random" id="NK=yF!p8ToxH%K*7^q}."><value name="min"><shadow type="math_number" id="^b{8(idIOa{nFc$26HDM"><field name="NUM">0</field></shadow></value><value name="limit"><shadow type="math_number" id="ZAH)6RWxvvi}bElKF^yS"><field name="NUM">10</field></shadow></value></block></value><value name="B"><shadow type="math_number" id=":L=13nTR%Kek26R}+Tv+"><field name="NUM">2</field></shadow></value></block></value><statement name="DO1"><block type="game_show_long_text" id="!6@IURq$dQa6?pP]VI%:"><field name="layout">DialogLayout.Full</field><value name="str"><shadow type="text" id=";Wd24|fZ^rK8p[A_=qkF"><field name="TEXT">Fact 12:There are dead zones in the oceans that have been created by pollution making life in those zones impossible for marine or plant life.</field></shadow></value></block></statement><value name="IF2"><block type="logic_compare" id="iVMO.2aWF^S.%?ipT4P6"><field name="OP">EQ</field><value name="A"><shadow type="math_number"><field name="NUM">0</field></shadow><block type="device_random" id="{II%!JGoHQ4B=w*]+kuF"><value name="min"><shadow type="math_number" id="1nucv{p+)$kn$Vi`1}G_"><field name="NUM">0</field></shadow></value><value name="limit"><shadow type="math_number" id="k~$$Wz.%UfDsgFo^pIA)"><field name="NUM">10</field></shadow></value></block></value><value name="B"><shadow type="math_number" id="0H;?ZeWvjvFQ}[f^,m#*"><field name="NUM">3</field></shadow></value></block></value><statement name="DO2"><block type="game_show_long_text" id="o22_,7@XYkC~vzU%P0_c"><field name="layout">DialogLayout.Full</field><value name="str"><shadow type="text" id="?_v-I:%hZ+3ktZ5kur~e"><field name="TEXT">Fact 13:Over 100,000 marine animals die every year from plastic entanglement and ingestion.</field></shadow></value></block></statement><value name="IF3"><block type="logic_compare" id="*@^theWQa%xYEp*Zpp7g"><field name="OP">EQ</field><value name="A"><shadow type="math_number"><field name="NUM">0</field></shadow><block type="device_random" id=".p%,eF5Y2*SXE@$*zSnt"><value name="min"><shadow type="math_number" id="Y^XI%7ncHnM0-aXBeJnL"><field name="NUM">0</field></shadow></value><value name="limit"><shadow type="math_number" id="z0%j8#944,gn|}xnPep,"><field name="NUM">10</field></shadow></value></block></value><value name="B"><shadow type="math_number" id="%I:pYvJv`?NpI4Cd+5*X"><field name="NUM">4</field></shadow></value></block></value><statement name="DO3"><block type="game_show_long_text" id="IvHMPWpn$41*R{H~`[~L"><field name="layout">DialogLayout.Full</field><value name="str"><shadow type="text" id="nPjujGR-5AAn%X)/pBA#"><field name="TEXT">Fact 14: Carbon emissions harm the oceans as well as the air. If our behavior continues as is, the surface water of the ocean could be 150% more acidic than it is now.</field></shadow></value></block></statement><value name="IF4"><block type="logic_compare" id="BODV7$jeAwx-8ht[MmoJ"><field name="OP">EQ</field><value name="A"><shadow type="math_number"><field name="NUM">0</field></shadow><block type="device_random" id="VwZukTl72Lz5}uADM%1k"><value name="min"><shadow type="math_number" id="X!;ZqpY2j^!3_Z!cDuMs"><field name="NUM">0</field></shadow></value><value name="limit"><shadow type="math_number" id="C[kq({h^Qu7jfy67~@`+"><field name="NUM">10</field></shadow></value></block></value><value name="B"><shadow type="math_number" id="c(HevaUO{;T0toscxo1U"><field name="NUM">5</field></shadow></value></block></value><statement name="DO4"><block type="game_show_long_text" id="5zry$eFu)c?{,a$4iatE"><field name="layout">DialogLayout.Full</field><value name="str"><shadow type="text" id="oymMk=z7u_4yZqj6k^nC"><field name="TEXT">Fact 15: Not all sewage that enters the ocean is treated. 80% of sewage that flows into the Mediterranean Sea is untreated, which can lead to disease.</field></shadow></value></block></statement><value name="IF5"><block type="logic_compare" id="3fF|e=fW7$U%zSXUbAnW"><field name="OP">EQ</field><value name="A"><shadow type="math_number"><field name="NUM">0</field></shadow><block type="device_random" id="Dldfg~P(#h?Y@%#oU+$$"><value name="min"><shadow type="math_number" id=",!tb1t@loRPN][.S$c|+"><field name="NUM">0</field></shadow></value><value name="limit"><shadow type="math_number" id="Ns4}:Oh8#IN^jUh0+2~a"><field name="NUM">10</field></shadow></value></block></value><value name="B"><shadow type="math_number" id="|[z^,Rz96+2g.oeNRgoG"><field name="NUM">6</field></shadow></value></block></value><statement name="DO5"><block type="game_show_long_text" id="_ZfY,_WRiFK%r@{f?]%("><field name="layout">DialogLayout.Full</field><value name="str"><shadow type="text" id="Gj=4}2fw)blrBD:t^6D:"><field name="TEXT">Fact 16: Oil spills only contribute to 12% of the oil in the ocean. 36% of the oil comes from runoff sources from cities and companies.</field></shadow></value></block></statement><value name="IF6"><block type="logic_compare" id="7=*Es-$g0)bP{wk]!,tf"><field name="OP">EQ</field><value name="A"><shadow type="math_number"><field name="NUM">0</field></shadow><block type="device_random" id="NeU^A~e}|U`xgc,##U5$"><value name="min"><shadow type="math_number" id="eFaFuihbi0!E.Ycn.)[o"><field name="NUM">0</field></shadow></value><value name="limit"><shadow type="math_number" id="g4v7D.k]ZgIl?$8oOjF6"><field name="NUM">10</field></shadow></value></block></value><value name="B"><shadow type="math_number" id="Iv2j1#)H!2JtAn(l:E!*"><field name="NUM">7</field></shadow></value></block></value><statement name="DO6"><block type="game_show_long_text" id="wnO%aA68`P{#O,+^@I[3"><field name="layout">DialogLayout.Full</field><value name="str"><shadow type="text" id="Pbrq-_dY(Pa^n~dL5jOG"><field name="TEXT">Fact 17: Ocean noise pollution is an issue, too. Ships, tankers, and shipping containers emit sounds like high-intensity sonar and air guns. This noise pollution injures fish, disrupts their habitats, and more.</field></shadow></value></block></statement><value name="IF7"><block type="logic_compare" id="?rMi8qujjv.:R:5Seetq"><field name="OP">EQ</field><value name="A"><shadow type="math_number"><field name="NUM">0</field></shadow><block type="device_random" id="dwhXG_qA-qk@u4l?TzLb"><value name="min"><shadow type="math_number" id="ahqa.w#ds)^rfEGw/YU7"><field name="NUM">0</field></shadow></value><value name="limit"><shadow type="math_number" id="I72H=(kfcBmVQI~sva1T"><field name="NUM">10</field></shadow></value></block></value><value name="B"><shadow type="math_number" id="$nz[v2:{M9`9qg|zF1;?"><field name="NUM">8</field></shadow></value></block></value><statement name="DO7"><block type="game_show_long_text" id="Ss}OBEg6)JV4Uia6lt]!"><field name="layout">DialogLayout.Full</field><value name="str"><shadow type="text" id="GPpVpXE(86-Fwx3wnYT^"><field name="TEXT">Fact 18: Over one-third of the Atlantic ocean that shellfish live in is negatively impacted by pollution. This adversely affects the shellfish businesses on the East Coast.</field></shadow></value></block></statement><value name="IF8"><block type="logic_compare" id="*SZ-DBO.#*mK{k-Rymm0"><field name="OP">EQ</field><value name="A"><shadow type="math_number"><field name="NUM">0</field></shadow><block type="device_random" id="f3?A#l:;0RxWkw9tO*.g"><value name="min"><shadow type="math_number" id="mAc5YbU/98cSn(?iJ,)F"><field name="NUM">0</field></shadow></value><value name="limit"><shadow type="math_number" id="G[=#B@t}bBw.mZ3X4q3="><field name="NUM">10</field></shadow></value></block></value><value name="B"><shadow type="math_number" id="Q0(N8@M##+(BStV#G;Hi"><field name="NUM">9</field></shadow></value></block></value><statement name="DO8"><block type="game_show_long_text" id="kfx(@*?fiJ6U4)LLYYe*"><field name="layout">DialogLayout.Full</field><value name="str"><shadow type="text" id="~]kks!8sK{Lr@neT[cZ?"><field name="TEXT">Fact 19: It’s estimated that by the year 2050, there will be more plastic in the ocean than fish (by weight).</field></shadow></value></block></statement><value name="IF9"><block type="logic_compare" id="_suR{j!=OndHaV$Eufaa"><field name="OP">EQ</field><value name="A"><shadow type="math_number"><field name="NUM">0</field></shadow><block type="device_random" id="u%s`U}/xTA4@7X?nj#sw"><value name="min"><shadow type="math_number" id="`d~d~vZ@Nf$3WRYvf2D!"><field name="NUM">0</field></shadow></value><value name="limit"><shadow type="math_number" id="8N6g-5?5@JA*_j{!{5~6"><field name="NUM">10</field></shadow></value></block></value><value name="B"><shadow type="math_number" id="6gh*6$/~yq%!;SLJU1KY"><field name="NUM">10</field></shadow></value></block></value><statement name="DO9"><block type="game_show_long_text" id="bYt7UxK+oo%gW^f1^r,:"><field name="layout">DialogLayout.Full</field><value name="str"><shadow type="text" id="@mIBR2[~Bk}}F,$#%7k5"><field name="TEXT">Fact 20: Fish and other marine life often can’t tell the difference between plastic and food. If the animal eats plastic, they can’t digest it. The plastic fills their stomachs, so they starve to death.</field></shadow></value></block></statement><statement name="ELSE"><block type="gameSplash" id="EtClO-%,=y{yvMFAX!*W"><mutation xmlns="http://www.w3.org/1999/xhtml" _expanded="0" _input_init="true"></mutation><value name="title"><shadow type="text" id=";/SH0{d^f$:La1`u[P=?"><field name="TEXT">Hi, who ever is looking at the code, it's currently 3:30am</field></shadow></value><value name="subtitle"><shadow type="text" id="ubN)Yf^EXf23g~GzM|/M"><field name="TEXT"></field></shadow></value></block></statement><nex